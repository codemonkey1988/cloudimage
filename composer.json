{
    "name": "codemonkey1988/cloudimage",
    "description": "PHP Client for cloudimage.io",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Tim Scheiner",
            "email": "schreiner.tim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Codemonkey1988\\Cloudimage\\": "src/Cloudimage/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codemonkey1988\\Cloudimage\\Tests\\": "tests/Cloudimage/Tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=ruleset.xml --extensions=php --encoding=utf-8 --tab-width=4 src"
    }
}
